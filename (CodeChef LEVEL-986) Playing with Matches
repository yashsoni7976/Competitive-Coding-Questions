#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,arr[10]={6,2,5,5,4,5,6,3,7,6};
	cin>>t;
	while(t--)
	{
	    int a,b,sum=0;
	    cin>>a>>b;
	    int c=a+b;
	    while(c!=0)
	    {
	        int temp=c%10;
	        sum+=arr[temp];
	        c/=10;
	    }
	    cout<<sum<<endl;
	}
	return 0;
}
