#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,a,b,c,d;
	cin>>t;
	while(t--)
	{
	    cin>>a>>b>>c>>d;
	    if(a>=b)
	        b+=c;
	    else if(a<b)
	        a+=c;
	        
	    if(a>=b)
	        b+=d;
    	else if(a<b)
    	    a+=d;
	        
	    if(a>=b)
	        cout<<"N\n";
	    else
	        cout<<"S\n";
	}
	return 0;
}
